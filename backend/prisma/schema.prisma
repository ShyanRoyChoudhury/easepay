generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  v         Int    @map("__v")
  firstname String
  lastname  String
  password  String
  username  String
}
